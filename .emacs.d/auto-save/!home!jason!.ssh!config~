# file:.ssh/config
Host *
     User jason
     ControlMaster auto
     ControlPath ~/.ssh/master-%r@%h:%p
     ForwardAgent yes
     ForwardX11 no
# TCP session.
ServerAliveInterval 60
# User kerberos
GSSAPIAuthentication yes
host sptunnel
    Hostname jump1.sto.spotify.net
    # Hostname jump1.ash.spotify.net
    DynamicForward localhost:1080
    ProxyCommand none 
    host *.spotify.net
    ProxyCommand ssh -qx sptunnel 'nc -w 600 $(echo %h|sed s/^+//) %p'


